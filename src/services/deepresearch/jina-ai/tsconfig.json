{
    "compilerOptions": {
      "target": "ES2020",
      "module": "node16",
      "outDir": "./dist",
      "rootDir": "./src",
      "sourceMap": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "resolveJsonModule": true
    }
  }
  